{
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "description": "Validate, view and explore json files offline with worrying about the data leaving monitor.",
  "default_locale": "en",
  "permissions": [],
  "content_scripts": [
    {
      "matches": [
        "https://music.youtube.com/*"
      ],
      "js": [
        "js/content-script.js"
      ]
    }
  ],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html?popup=true",
    "default_title": "__MSG_extName__",
    "default_icon": {
      "64": "icons/64.png"
    }
  }
}