{
  "manifest_version": 2,
  "name": "offline-json",
  "version": "0.1.0",
  "description": "Validate, view and explore json files 'offline' without worrying about the data leaving monitor.",
  "default_locale": "en",
  "permissions": [],
  "content_scripts": [],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "background": {},
  "browser_action": {
    "default_popup": "popup.html?popup=true",
    "default_title": "offline-json",
    "default_icon": {
      "64": "icons/64.png"
    }
  }
}